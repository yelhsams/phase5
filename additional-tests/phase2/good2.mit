print(1 + 2 + 3);       // (1+2)+3 = 6
print(1 - 2 - 3);       // (1-2)-3 = -4
print(2 * 3 * 4);       // (2*3)*4 = 24
print(100 / 10 / 2);    // (100/10)/2 = 5
print(10 - 3 + 1);      // (10-3)+1 = 8
print(100 / 5 * 2);     // (100/5)*2 = 40
print(100 - 2 * 3);      // 100 - (2 * 3) = 94
// sanity checks with parentheses (should differ)
print(1 - (2 - 3));     // 2
print(100 / (10 * 2));  // 5
