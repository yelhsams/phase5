lshift = fun(a, b) {
    result = a;
    i = 0;
    while (i < b) {
        result = result * 2;
        i = i + 1;
    }
    return result;
};

mod = fun(a, b) {
    return a - (a / b) * b;
};

nsieve = fun(n) {
    count = 0;
    flags = {};
    i = 0;
    while (i < n) {
        flags[i] = true;
        i = i + 1;
    }

    i = 2;
    while (i < n) {
        if (flags[i]) {
            count = count + 1;
            j = i * 2;
            while (j < n) {
                flags[j] = false;
                j = j + i;
            }
        }
        i = i + 1;
    }

    print("Primes up to\t" + n + "\t" + count);
};

n = intcast(input());

i = 0;
while (i < 3) {
    shift_amount = n - i;
    sieve_size = lshift(10000, shift_amount);
    nsieve(sieve_size);
    i = i + 1;
}

